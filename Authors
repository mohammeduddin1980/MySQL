CREATE TABLE Authors(
Auth_ID INT NOT NULL AUTO_INCREMENT PRIMARY KEY,
Last_Name VARCHAR(100) NOT NULL,
Firs_Name VARCHAR(100),
Phone_Number VARCHAR(50) NOT NULL,
Email VARCHAR(50)
);



  
