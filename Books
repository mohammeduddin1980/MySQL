CREATE TABLE Books(
Book_ID INT NOT NULL AUTO_INCREMENT PRIMARY KEY,
Book_Title VARCHAR(100) NOT NULL,
Publishers VARCHAR(100),
Publish_Date VARCHAR(50) NOT NULL,
Edition VARCHAR(50) NOT NULL
);





